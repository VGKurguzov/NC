<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.8.0//EN"
        "http://gwtproject.org/doctype/2.8.0/gwt-module.dtd">
<module rename-to="HistoryClass">

    <inherits name='com.google.gwt.user.User'/>
    <inherits name="com.google.gwt.logging.Logging"/>

    <entry-point class='com.historyClass.client.HistoryClass'/>

    <set-property name = "gwt.logging.logLevel" value="SEVERE"/>
    <set-property name = "gwt.logging.enabled" value = "TRUE"/>
    <!--<set-property name="gwt.logging.popupHandler" value="DISABLED" />-->
    <!-- Property 'gwt.logging.popupLogHandler' not found -->

    <servlet path='/HistoryClassService' class='com.historyClass.server.HistoryClassServiceImpl'/>

</module>